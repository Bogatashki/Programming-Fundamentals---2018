using System;

namespace _13._Vowel_or_Digit
{
    class Program
    {
        static void Main(string[] args)
        {          
            char simbol = char.Parse(Console.ReadLine());
            if (simbol == 'a' || simbol == 'o' || simbol == 'e' || simbol == 'y' || simbol == 'i' || simbol == 'u')
            {
                Console.WriteLine($"vowel");
            }
            else if (char.IsDigit(simbol))
            {
                Console.WriteLine($"digit");
            }
            else
            {
                Console.WriteLine($"other");
            }
        }
    }
}
