using System;

namespace _03._Practice_Chars_And_Strings
{
    class Program
    {
        static void Main(string[] args)
        {
            string soft = Console.ReadLine();
            char b = char.Parse(Console.ReadLine());
            char y = char.Parse(Console.ReadLine());
            char e = char.Parse(Console.ReadLine());
            string love = Console.ReadLine();
            Console.WriteLine(soft);
            Console.WriteLine(b);
            Console.WriteLine(y);
            Console.WriteLine(e);
            Console.WriteLine(love);

        }
    }
}
